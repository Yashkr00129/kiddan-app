{
	"expo": {
		"name": "Kiddaan News",
		"slug": "kiddan-news",
		"version": "2.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/kiddan_app_logo_android.jpg",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.yashkr00129.kiddannews",
			"versionCode": 2,
			"googleServicesFile": "./google-services.json"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-notifications",
				{
					"icon": "./assets/images/kiddan_app_logo_android.jpg",
					"color": "#ffffff",
					"defaultChannel": "default"
				}
			],
			"expo-video",
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/Roboto-Regular.ttf"]
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "0c0aaa33-309d-4fb0-a8e6-28dddad0e894"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/0c0aaa33-309d-4fb0-a8e6-28dddad0e894"
		}
	},
	"Transform": "AWS::Serverless-2016-10-31"
}
