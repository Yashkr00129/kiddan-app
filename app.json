{
	"expo": {
		"name": "Kiddaan News",
		"slug": "kiddan-news",
		"version": "2.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/kiddan_app_logo_android.jpg",
		"scheme": "kiddaanapp",
		"userInterfaceStyle": "automatic",
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#8c007f"
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"package": "com.yashkr00129.kiddannews",
			"versionCode": 2,
			"googleServicesFile": "./google-services.json",
			"intentFilters": [
				{
					"action": "VIEW",
					"autoVerify": true,
					"data": [
						{
							"scheme": "https",
							"host": "*.kiddaanapp.com",
							"pathPrefix": "/articles"
						}
					],
					"category": ["BROWSABLE", "DEFAULT"]
				}
			]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-notifications",
				{
					"icon": "./assets/images/kiddan_app_logo_android.jpg",
					"color": "#ffffff",
					"defaultChannel": "default"
				}
			],
			"expo-video",
			[
				"expo-font",
				{
					"fonts": ["./assets/fonts/Roboto-Regular.ttf"]
				}
			],
			[
				"react-native-share",
				{
					"ios": ["fb", "instagram", "twitter", "tiktoksharesdk"],
					"android": [
						"com.facebook.katana",
						"com.instagram.android",
						"com.twitter.android",
						"com.zhiliaoapp.musically"
					]
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"eas": {
				"projectId": "0c0aaa33-309d-4fb0-a8e6-28dddad0e894"
			}
		},
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/0c0aaa33-309d-4fb0-a8e6-28dddad0e894"
		}
	},
	"Transform": "AWS::Serverless-2016-10-31"
}
